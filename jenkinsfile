pipeline {
    agent any
tools 
{
maven 'M2_HOME'
}
 environment { 

        registry = "waelbannani/tpachat" 

        registryCredential = 'dockerhub' 
        
            
        dockerImage = '' 

    }
    
    stages {
        stage('Checkout GIT') {
            steps {
                git branch:'wael',
                url:'https://github.com/waelbannani22/tpAchatProject'
                
              

            }
        }
        stage('MVN CLEAN')
        {
        steps {
        sh """mvn -version"""
        sh 'mvn clean'
        }
        
        }
        stage('MVN COMPILE')
        {
        steps {
        sh 'mvn compile'
        }
        }
        
              stage('MVN INSTALL')
        {
        steps {
        sh 'mvn package'
        }
        }
       
       
       
        /*stage('MVN SONARQUBE')
        {
        steps{
        sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=esprit'
        }
        }
             stage('MVN INSTALL')
        {
        steps {
        sh 'mvn package'
        }
        }
         stage('MVN NEXUS')
        {
        steps {
        sh 'mvn clean deploy'
        }
        }
        */
        stage('Docker compose ')
        {
        steps {
        sh 'sudo docker-compose up'
        }
        }
       


        
    }
    
}


pipeline {
    agent any

 environment { 

        registry = "waelbannani/tpachat" 

        registryCredential = 'dockerhub' 
        
            
        dockerImage = '' 

    }
    
    stages {
        stage('Checkout GIT') {
            steps {
                git branch:'wael',
                url:'https://github.com/waelbannani22/tpAchatProject'
                
              

            }
        }
        stage('MVN CLEAN')
        {
        steps {
        sh """mvn -version"""
        sh 'mvn clean'
        }
        
        }
       
        
              stage('MVN INSTALL')
        {
        steps {
        sh 'mvn package'
        }
        }
        stage('MVN NEXUS')
        {
        steps {
        sh 'mvn clean deploy'
        }
        }
       
       
      
        stage('Docker compose ')
        {
        steps {
        sh 'sudo docker-compose up'
        }
        }
       


        
    }
    
}

